{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/pages/*": ["pages/*"],
      "@/components/*": ["components/*"],
      "@/models": ["models/"],
      "@/models/*": ["models/*"],
      "@/constants": ["constants/"],
      "@/constants/*": ["constants/*"],
      "@/store": ["store/"],
      "@/store/*": ["store/*"],
      "@/utils": ["utils/"],
      "@/utils/*": ["utils/*"],
      "@/api": ["api/"],
      "@/api/*": ["api/*"],
      "@/icons/*": ["icons/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "next.config.js"],
  "exclude": ["node_modules"]
}
